	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To evaluate hypoxemia in newborn infants with respiratory distress.">
			keywords = <"Downe score, neonates, newborn, hypoxemia, respiratory distress", ...>
			use = <"Used to record the results for each component parameter and their total sum for the Downe score.

Downe score is used for assessing hypoxemia in neonates with respiratory distress. Downes score can be used as an alternative to assess respiratory distress clinically and determine the need of oxygen supplementation when pulse oximetry or blood gas analysis are not available.
">
			misuse = <"For preterms with Hyaline Membrane Disease, an assessment using the Silverman Anderson Retraction Score is more suitable.">
		>
	>
	other_details = <
		["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 4.0 International License. To view a copy of this license, visit http://creativecommons.org/licenses/by-sa/4.0/.">
		["custodian_organisation"] = <"openEHR Foundation">
		["references"] = <"Downes, J. J., Vidyasagar, D., Morrow, G. M., & Boggs, T. R. (1970). Respiratory Distress Syndrome of Newborn Infants: I. New Clinical Scoring System (RDS Score) with Acid-Base and Blood- Gas Correlations. Clinical Pediatrics, 9(6), 325–331. https://doi.org/10.1177/000992287000900607

Mathai, S., Raju, U., & Kanitkar, M. (2007). Management of Respiratory Distress in the Newborn. Medical Journal, Armed Forces India, 63(3), 269–272. https://doi.org/10.1016/S0377-1237(07)80152-3

Rusmawati, A., Haksari, E. L., & Naning, R. (2008). Downes score as a clinical assessment for hypoxemia in neonates with respiratory distress. Paediatrica Indonesiana, 48(6), 342–345. https://doi.org/10.14238/pi48.6.2008.342-5

Shrestha, S., Shrestha, S.K. , Shrestha, G.S. , & Dhakal, M.S. (2021). Assessment of Hypoxemia using Downes Score in Neonates with Respiratory Distress. Nepal Medical College Journal, 23, 194–197. https://doi.org/10.3126/nmcj.v23i3.40375">
